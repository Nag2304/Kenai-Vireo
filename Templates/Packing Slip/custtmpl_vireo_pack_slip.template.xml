<?xml version="1.0"?>
<!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
	<link name="NotoSans" type="font" subtype="truetype" src="${nsfont.NotoSans_Regular}" src-bold="${nsfont.NotoSans_Bold}" src-italic="${nsfont.NotoSans_Italic}" src-bolditalic="${nsfont.NotoSans_BoldItalic}" bytes="2" />
	<#if .locale == "zh_CN">
		<link name="NotoSansCJKsc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKsc_Regular}" src-bold="${nsfont.NotoSansCJKsc_Bold}" bytes="2" />
	<#elseif .locale == "zh_TW">
		<link name="NotoSansCJKtc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKtc_Regular}" src-bold="${nsfont.NotoSansCJKtc_Bold}" bytes="2" />
	<#elseif .locale == "ja_JP">
		<link name="NotoSansCJKjp" type="font" subtype="opentype" src="${nsfont.NotoSansCJKjp_Regular}" src-bold="${nsfont.NotoSansCJKjp_Bold}" bytes="2" />
	<#elseif .locale == "ko_KR">
		<link name="NotoSansCJKkr" type="font" subtype="opentype" src="${nsfont.NotoSansCJKkr_Regular}" src-bold="${nsfont.NotoSansCJKkr_Bold}" bytes="2" />
	<#elseif .locale == "th_TH">
		<link name="NotoSansThai" type="font" subtype="opentype" src="${nsfont.NotoSansThai_Regular}" src-bold="${nsfont.NotoSansThai_Bold}" bytes="2" />
	</#if>
    <macrolist>
        <macro id="nlheader">
            
<table class="header" style="width: 100%; font-size: 10pt;"><tr>
<td><#if subsidiary?? && subsidiary.logo?length != 0> <@filecabinet nstype="image" style="float: left; margin: 7px;" src="${subsidiary.logo@url}" width="200" height="75" /> <#else> <#if companyInformation?? && companyInformation.logoUrl?length != 0> <@filecabinet nstype="image" style="float: left; margin: 7px;" src="${companyInformation.logoUrl}" /> </#if> </#if></td>
<td style="padding-left: 80px; padding-top: 25px;"><span class="title">${record@title}</span></td>
</tr>
<tr>
<td align="left">${record.custbody_atlas_hb_std_salesorderloc.name?substring(record.custbody_atlas_hb_std_salesorderloc.name?last_index_of('-') + 1)}&nbsp;${record.custbody_atlas_hb_std_salesorderloc.locationtype}<span class="nameandaddress"><br />${record.custbody_vireo_subsidiary_name}</span><br /><span class="nameandaddress">${record.custbody_subsidiary_address}</span></td>
<td style="padding-left: 82px; padding-bottom: 20px;">
<table style="width: 98.9395%;"><tr>
<td style="width: 41.4628%;"><strong>Order Date</strong></td>
<td style="width: 58.5406%;">${record.trandate}</td>
</tr>
<tr>
<td style="padding-top: 10px; padding-right: 15px; width: 41.4628%;"><strong>Order #</strong></td>
<td style="padding-top: 10px; width: 58.5406%;">${record.createdfrom.tranid}</td>
</tr>
<tr>
<td style="padding-top: 10px; width: 41.4628%;"><strong>Customer Order #</strong></td>
<td style="padding-top: 10px; width: 58.5406%;">${record.createdfrom.otherrefnum}</td>
</tr>
<tr>
<td style="padding-top: 10px; width: 41.4628%;"><strong>Ship Date</strong></td>
<td style="padding-top: 10px; width: 58.5406%;">${record.shippeddate}</td>
</tr>
<tr>
<td style="padding-top: 10px; width: 41.4628%;"><strong>Tracking #</strong></td>
<td style="padding-top: 10px; width: 58.5406%;">${salesorder.linkedtrackingnumbers}</td>
</tr>
<tr>
<td style="padding-top: 10px; width: 41.4628%;"><strong>Ship Via</strong></td>
<td style="padding-top: 10px; width: 58.5406%;">${record.shipmethod}</td>
</tr></table>
</td>
</tr></table>

        </macro>
        <macro id="nlfooter">
            
<table class="footer"><tr><#if preferences.PRINT_BARCODES>
<td><barcode codetype="code128" showtext="true" value="${record.tranid}"/></td>
</#if>
<td align="right"><pagenumber/> of <totalpages/></td>
</tr></table>

        </macro>
    </macrolist>
    <style>
    * {
        <#if .locale == "zh_CN">
            font-family: NotoSans, NotoSansCJKsc, sans-serif;
        <#elseif .locale == "zh_TW">
            font-family: NotoSans, NotoSansCJKtc, sans-serif;
        <#elseif .locale == "ja_JP">
            font-family: NotoSans, NotoSansCJKjp, sans-serif;
        <#elseif .locale == "ko_KR">
            font-family: NotoSans, NotoSansCJKkr, sans-serif;
        <#elseif .locale == "th_TH">
            font-family: NotoSans, NotoSansThai, sans-serif;
        <#else>
            font-family: NotoSans, sans-serif;
        </#if>
    }
		table {
			font-size: 9pt;
			table-layout: fixed;
		}
        th {
            font-weight: bold;
            font-size: 8pt;
            vertical-align: middle;
            padding: 5px 6px 3px;
            background-color: #e3e3e3;
            color: #333333;
        }
        td {
            padding: 4px 6px;
        }
		td p { align:left }
        b {
            font-weight: bold;
            color: #333333;
        }
        table.header td {
            padding: 0;
            font-size: 10pt;
        }
        table.footer td {
            padding: 0;
            font-size: 8pt;
        }
        table.itemtable th {
            padding-bottom: 10px;
            padding-top: 10px;
            border: 1px solid #777777;
          color:#FFFFFF;
        }

      table.itemtable td {
         border: 1px solid #777777;
      }

        table.body td {
            padding-top: 2px;
          
        }
        td.addressheader {
            font-size: 8pt;
            font-weight: bold;
            padding-top: 6px;
            padding-bottom: 2px;
        }
        td.address {
            padding-top: 0;
        }
        span.title {
            font-size: 18pt;
            font-weight: bold;
        }
        span.number {
            font-size: 16pt;
        }
        span.itemname {
            font-weight: bold;
            line-height: 150%;
        }
        div.returnform {
            width: 100%;
            /* To ensure minimal height of return form */
            height: 200pt;
            page-break-inside: avoid;
            page-break-after: avoid;
        }
        hr {
            border-top: 1px dashed #d3d3d3;
            width: 100%;
            color: #ffffff;
            background-color: #ffffff;
            height: 1px;
        }
</style>
</head>
<body header="nlheader" header-height="28%" footer="nlfooter" footer-height="20pt" padding="0.5in 0.5in 0.5in 0.5in" size="Letter">
    <table style="width: 100%; margin-top: 10px;"><tr>
<td class="addressheader">${salesorder.shipaddress@label}</td>
</tr>
<tr>
<td class="address">${salesorder.shipaddress}</td>
</tr></table>
<#if salesorder.item?has_content>
<table class="itemtable" style="width: 100%; margin-top: 10px; border-collapse: collapse;">
<thead>
<tr>
<th colspan="4">${salesorder.item[0].item@label}</th>
<th colspan="6">Item Description</th>
<th colspan="3" align="right">Units</th>
<th colspan="4">Lot#</th>
<th colspan="4">Exp.Date</th>
<th colspan="3" align="right">Order Qty</th>
<th colspan="3" align="right">Shipped Qty</th>
</tr>
</thead>
<#list salesorder.item as tranline><tr>
<td colspan="4">${tranline.item}</td>
<td colspan="6">${tranline.description}</td>
<td colspan="3" align="right">${tranline.units}</td>
<td colspan="4">${tranline.custcol_vireo_lot_serial_number}</td>
<td colspan="4">${tranline.custcol_vireo_lot_exp_date}</td>
<td colspan="3" align="right">${tranline.quantityordered}</td>
<td colspan="3" align="right">${tranline.quantity}</td>
</tr>
</#list></table>
</#if> <#if preferences.RETURNFORM && returnForm??><hr />
<div class="returnform">
<table style="width: 100%; margin-top: 10px;"><tr>
<td style="width: 49.5507%;"><strong>${record.custbody_vireo_subsidiary_name}</strong></td>
<td style="text-align: left; width: 50.5311%;"><strong><span class="number">${returnForm@title}</span></strong></td>
</tr></table>
<table style="width: 100%; margin-top: 10px; border-collapse: collapse;"><tr>
<td style="width: 50.194%;" colspan="4"><strong>Ship Returns To</strong><br />${record.custbody_atlas_hb_std_salesorderloc.name?substring(record.custbody_atlas_hb_std_salesorderloc.name?last_index_of('-') + 1)}&nbsp;${record.custbody_atlas_hb_std_salesorderloc.locationtype}<br />${record.custbody_subsidiary_address}</td>
<td style="width: 49.7712%;">
<table><tr>
<td><strong>${returnForm.rmaNum}</strong></td>
<td>&nbsp;</td>
</tr>
<tr>
<td><strong>${returnForm.customerName}</strong></td>
<td>${salesorder.entity}</td>
</tr>
<tr>
<td><strong>${salesorder.tranid@label}</strong></td>
<td>${salesorder.tranid}</td>
</tr></table>
</td>
</tr></table>
<table class="itemtable" style="width: 100%; margin-top: 10px;">
<thead>
<tr>
<th colspan="2">${returnForm.item}</th>
<th colspan="1">${returnForm.quantity}</th>
<th colspan="5">${returnForm.reason}</th>
</tr>
</thead><tr>
<td colspan="8">&nbsp;</td>
</tr></table>
</div>
</#if>
</body>
</pdf>